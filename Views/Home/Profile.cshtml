@model User

<div class="text-center">
    <a href="/products"><p>Products</p></a>
    <h1 class="display-4">@Model.FirstName @Model.LastName</h1>
      </div>
      <div>Reservations:</div>
      <ul>
        @foreach(ReservedLane resLane in @Model.Lanes)
        {
          <li>@resLane.Lane.LaneNumber</li>
        }
      </ul>
      <div>Leagues:</div>
      <ul>
        @foreach(LeagueMember league in @Model.Leagues)
        {
          <li>@league.League.Name</li>
        }
      </ul>
    </div>
    <div class="card-body">
</div>
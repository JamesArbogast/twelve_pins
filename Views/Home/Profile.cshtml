@model User

<div class="text-center d-flex-column m-4" >
    <h1 class="display-4">@Model.FirstName @Model.LastName</h1>
      </div>
      <h3 class="m-4">Lane Reservations</h3>
      <table class="m-4 px-3" style="border: 3px black solid; padding: 20px">
        <thead class="m-4">
          <th>Time</th>
          <th>Lane</th>
        </thead>
        @foreach(ReservedLane resLane in @Model.Lanes)
        {
          <td>@resLane.ReserveDate</td>
          <td>@resLane.ReservedLaneId</td>
        }
      </table>
      <h3 class="m-4">Leagues</h3>
      <ul>
      </ul>
    </div>
    <div class="card-body">
</div>
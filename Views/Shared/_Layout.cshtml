<body>
    <nav class="navbar sticky-top navbar-expand-lg navbar-dark w-100 p-0">
            <div class="container-fluid">
                <a class="navbar-brand w-50 h-100 p-0" href="#">
                <img id="nav-logo" src="/img/logo.png" alt="" width="170" height="90" class="d-inline-block align-text-center overflow-hidden">
                </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse w-25" id="navbarNavDropdown">
            <ul class="navbar-nav w-50">
                <li class="nav-item">
                    <a class="nav-link font-bold mr-5" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link mr-3" asp-area="" asp-controller="Event" asp-action="EatPage">Eat</a>
                </li>
                <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle mr-3" href="#" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                    Bowl
                </a>
                <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                    <li><a class="dropdown-item mr-3" asp-area="" asp-controller="Event" asp-action="ViewReserveLane">Reserve a Lane</a></li>
                    <li><a class="dropdown-item mr-3" asp-area="" asp-controller="Event" asp-action="ViewReserveLane">Birthday</a></li>
                    <li><a class="dropdown-item mr-3" asp-area="" asp-controller="Event" asp-action="ViewReserveLane">Company Event</a></li>
                    <li><a class="dropdown-item mr-3" asp-area="" asp-controller="Event" asp-action="Specials">Specials</a></li>
                </ul>
                </li>
                <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle mr-3" href="#" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                    Leagues
                </a>
                <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                    <li><a class="dropdown-item mr-3 " asp-area="" asp-controller="Event" asp-action="LeaguePage">Game Schedule</a></li>
                    <li><a class="dropdown-item mr-3 " asp-area="" asp-controller="Event" asp-action="LeaguePage">Tournaments</a></li>
                    <li><a class="dropdown-item mr-3 " asp-area="" asp-controller="Event" asp-action="LeaguePage">League Standings</a></li>
                    <li><a class="dropdown-item mr-3 " asp-area="" asp-controller="Event" asp-action="Shop">Pro Shop</a></li>
                </ul>
                </li>
                    @if(Context.Session.GetInt32("UserId") == 1 || Context.Session.GetInt32("UserId") == 2)
                    {
                        <li class="dropdown nav-item">
                            <a class="nav-link mr-3" asp-area="" asp-controller="Event" asp-action="Admin">Admin</a>
                        </li>
                    }

                    @if (Context.Session.GetInt32("UserId") != null)
                    {
                        <li class="nav-item">
                            <a class="nav-link mr-3" asp-controller="Home" asp-action="Logout">Logout (@Context.Session.GetString("FirstName"))</a>
                        </li>
                    }
                    
                    else 
                    {
                    <li class="nav-item">
                        <a class="nav-link mr-3" asp-area="" asp-controller="Home" asp-action="SignIn">Login</a>
                    </li>
                    }
                    @if (Context.Session.GetInt32("UserId") != null)
                    {
                        <li class="nav-item">
                            <a class="nav-link mr-3" asp-controller="Home" asp-action="Profile" asp-route-userId="@Context.Session.GetInt32("UserId")">Profile</a>
                        </li>
                    }
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Event" asp-action="About" >About</a>
                    </li>
            </ul>
            </div>
        </div>
    </nav>



